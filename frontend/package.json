{
  "name": "terminal-app",
  "version": "1.0.0",
  "description": "React app with SSH terminal connection",
  "main": "index.js",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "backend": "cd backend && npm start",
    "dev": "concurrently \"npm start\" \"npm run backend\"",
    "install-backend": "cd backend && npm install",
    "docker:build": "docker build -t bpmn-frontend .",
    "docker:run": "docker run -p 3000:80 bpmn-frontend"
  },
  "proxy": "http://localhost:5000",
  "dependencies": {
    "bpmn-js": "^18.7.0",
    "cors": "^2.8.5",
    "diagram-js": "^15.4.0",
    "node-pty": "^1.0.0",
    "pg": "^8.16.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.0",
    "react-scripts": "5.0.1",
    "reactflow": "^11.11.4",
    "ssh2": "^1.17.0",
    "ws": "^8.18.3",
    "xterm": "^5.3.0",
    "xterm-addon-attach": "^0.9.0",
    "xterm-addon-fit": "^0.8.0",
    "xterm-addon-web-links": "^0.9.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "concurrently": "^7.6.0"
  }
}
